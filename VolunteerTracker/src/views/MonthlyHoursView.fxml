<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="800.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1" fx:controller="views.MonthlyHoursViewController">
    <stylesheets>
        <URL value="@volunteer.css" />
    </stylesheets>
   <children>
      <VBox alignment="TOP_CENTER" prefHeight="473.0" prefWidth="800.0">
         <children>
            <Label alignment="TOP_CENTER" contentDisplay="CENTER" styleClass="header" text="Hours Worked" />
            <BarChart fx:id="barChart">
              <xAxis>
                <CategoryAxis fx:id="months" label="Months" side="BOTTOM" />
              </xAxis>
              <yAxis>
                <NumberAxis fx:id="hoursWorked" label="Hours Worked" side="LEFT" />
              </yAxis>
            </BarChart>
         </children>
      </VBox>
      <Button layoutX="740.0" layoutY="555.0" mnemonicParsing="false" text="Back" />
   </children>
</AnchorPane>
